{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "sample_rp_swagger_generator",
      "program": "f:/adl/cli/dist/index.js",
      "args": [
        "--pre-load-apis=name=sample_rp+path=d:\\adl\\sample_rp",
        "--pre-load-runtimes=path=d:\\adl\\arm.adl",
        "-a=verbose",
        "generators",
        "--action=run",
        "--generator-name=arm.swagger",
        "--config=\"key=value\""
      ],
      "smartStep": false,
      "skipFiles": [
        "<node_internals>/**/*.js"
      ],
      "cwd": "${workspaceRoot}"
    },
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "normalize",
      "program": "f:/adl/cli/dist/index.js",
      "args": [
        "machinery",
        "--api-name=computeapi",
        "--action=normalize",
        "--api-version=2018-10-01",
        "--versioned-api-type-name=vmscaleset20181001",
        "--source=f:/adl/docs_ex/compute-rp-sample-data/vmscaleset_2018-10-01.json"
      ],
      "smartStep": false,
      "skipFiles": [
        "<node_internals>/**/*.js"
      ],
      "cwd": "${workspaceRoot}"
    },
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "denormalize",
      "program": "f:/adl/cli/dist/index.js",
      "args": [
        "machinery",
        "--api-name=computeapi",
        "--action=denormalize",
        "--target-api-version=2018-10-01",
        "--target-versioned-api-type-name=vmscaleset20181001",
        "--versioned-api-type-name=vmscaleset20181001",
        "--source=f:/adl/docs_ex/compute-rp-sample-data/vmscaleset-normalized.json"
      ],
      "smartStep": false,
      "skipFiles": [
        "<node_internals>/**/*.js"
      ],
      "cwd": "${workspaceRoot}"
    }
  ]
}